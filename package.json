{
  "name": "webservices-delaware",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "nodemonConfig": {
    "signal": "SIGTERM",
    "ignore": [
      "node_modules/*",
      ".vscode/*"
    ],
    "delay": "80",
    "watch": [
      "config",
      "src"
    ],
    "ext": "js,json",
    "legacyWatch": true,
    "exec": "node --inspect=0.0.0.0:9001 --trace-warnings src/index.js"
  },
  "scripts": {
    "start": "npx env-cmd nodemon",
    "start:prod": "node src/index.js",
    "test": "npx env-cmd -f .env.test jest",
    "test:coverage": "npx env-cmd -f .env.test jest --coverage",
    "test:ci": "jest",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "bcrypt": "^5.1.0",
    "config": "^3.3.6",
    "env-cmd": "^10.1.0",
    "jest": "^29.3.1",
    "jest-each": "^29.3.1",
    "joi": "^17.4.2",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.3.0",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.3.0",
    "mysql2": "^2.3.0",
    "node-emoji": "^1.11.0",
    "pbkdf2": "^3.1.2",
    "serialize-error": "8.1.0",
    "supertest": "^6.3.1",
    "winston": "^3.3.3",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
